{
   "$schema": "http://json-schema.org/draft-04/schema#",
   "title": "Process",
   "description": "Process resource",
   "type": "object",
   "properties": {
     "processId":{
       "description": "A unique numeric ID of the process.",
       "type": "string"
     },
     "processLabel":{
       "description": "A uniqe label for the process.",
       "type": "string"
     },
     "name":{
       "description": "The name of the process.",
       "type": "string"
     },
     "status":{
       "description": "The process status, one of 'ENABLED', 'DISABLED'",
       "type": "string"
     }
   }
}